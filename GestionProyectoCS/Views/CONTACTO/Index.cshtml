@model IEnumerable<GestionProyectoCS.Models.Datos.CONTACTO>

@{
    ViewBag.Title = "Listado De Contactos";
}
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css">
<div class="app-main">
    @Html.Partial("_SideBar")
    <div class="app-main__outer">
        <div class="app-main__inner">
            <div class="app-page-title">
                <div class="page-title-wrapper">
                    <div class="page-title-heading">
                        <div class="page-title-icon">
                            <i class="pe-7s-notebook icon-gradient bg-warm-flame">
                            </i>
                        </div>
                        <div>
                            Contactos
                            <div class="page-title-subheading">
                                Listado de Contactos
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-content">
                <div class="tab-pane tabs-animation fade show active" id="tab-content-0" role="tabpanel">
                    <div class="main-card mb-3 card">
                        <div class="card-body">
                            <p>
                                @Html.ActionLink("Nuevo Contacto", "Create")
                            </p>
                            <table id="contacto" class="table table-striped table-bordered" style="width:100%">
                                <thead>
                                    <tr>
                                        <th>
                                            Nombres
                                        </th>
                                        <th>
                                            Apellidos
                                        </th>
                                        <th>
                                            Responsable
                                        </th>
                                        <th>
                                            Cliente
                                        </th>
                                        <th>
                                            Telefono1
                                        </th>
                                        <th>
                                            Telefono2
                                        </th>
                                        <th>
                                            Extencion
                                        </th>
                                        <th>
                                            Correo1
                                        </th>
                                        <th>
                                            Correo2
                                        </th>
                                        <th>
                                            Estado
                                        </th>
                                        <th>
                                            Fecha Registro
                                        </th>

                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.NOMBRES)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.APELLIDOS)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.RESPONSABLE.DESCRIPCION)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CLIENTE.CLIENTE1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TELF1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TELF2)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.EXT)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.EMAIL1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.EMAIL2)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ACTIVO)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FECHA_CREACION)
                                        </td>
                                        <td>
                                            @Html.ActionLink("Editar", "Edit", new { id = item.ID_CONTACTO }) |
                                            @Html.ActionLink("Eliminar", "Delete", new { id = item.ID_CONTACTO })
                                        </td>
                                    </tr>
                                    }
                                </tbody>
                                <tfoot>
                                    <tr>
                                        <th>
                                            Nombres
                                        </th>
                                        <th>
                                            Apellidos
                                        </th>
                                        <th>
                                            Responsable
                                        </th>
                                        <th>
                                            Cliente
                                        </th>
                                        <th>
                                            Telefono1
                                        </th>
                                        <th>
                                            Telefono2
                                        </th>
                                        <th>
                                            Extencion
                                        </th>
                                        <th>
                                            Correo1
                                        </th>
                                        <th>
                                            Correo2
                                        </th>
                                        <th>
                                            Estado
                                        </th>
                                        <th>
                                            Fecha Registro
                                        </th>
                                        <th></th>
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts
{
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.js"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#contacto').DataTable();
        });
    </script>
}

