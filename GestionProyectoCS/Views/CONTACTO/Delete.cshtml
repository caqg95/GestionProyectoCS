@model GestionProyectoCS.Models.Datos.CONTACTO

@{
    ViewBag.Title = "Eliminar Responsabilidad";
}
<div class="app-main">
    @Html.Partial("_SideBar")
    <div class="app-main__outer">
        <div class="app-main__inner">
            <div class="app-page-title">
                <div class="page-title-wrapper">
                    <div class="page-title-heading">
                        <div class="page-title-icon">
                            <i class="pe-7s-study icon-gradient bg-warm-flame">
                            </i>
                        </div>
                        <div>
                            Contactos
                            <div class="page-title-subheading">
                                Eliminar contacto
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-content">
                <div class="tab-pane tabs-animation fade show active" id="tab-content-0" role="tabpanel">
                    <div class="main-card mb-3 card">
                        <div class="card-body">
                            <h3>Esta seguro de eliminar este contacto?</h3>
                            <div>
                                <hr />
                                <dl class="dl-horizontal">
                                    <dt>
                                        Nombres
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.NOMBRES)
                                    </dd>

                                    <dt>
                                        Apellidos
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.APELLIDOS)
                                    </dd>

                                    <dt>
                                        Telefono1
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.TELF1)
                                    </dd>

                                    <dt>
                                        Telefono2
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.TELF2)
                                    </dd>

                                    <dt>
                                        Extencion
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.EXT)
                                    </dd>

                                    <dt>
                                        Correo1
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.EMAIL1)
                                    </dd>

                                    <dt>
                                        Correo2
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.EMAIL2)
                                    </dd>

                                    <dt>
                                        Estado
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.ACTIVO)
                                    </dd>

                                    <dt>
                                        Usuario Creacion
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.USUARIO_CREACION)
                                    </dd>

                                    <dt>
                                        Fecha Creacion
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.FECHA_CREACION)
                                    </dd>

                                    <dt>
                                        Usuario Modificacion
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.USUARIO_MODIFICACION)
                                    </dd>

                                    <dt>
                                        Fecha Modificacion
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.FECHA_MODIFICACION)
                                    </dd>

                                    <dt>
                                        Cliente
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.CLIENTE.CLIENTE1)
                                    </dd>

                                    <dt>
                                        Responsable
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.RESPONSABLE.DESCRIPCION)
                                    </dd>

                                </dl>

                                @using (Html.BeginForm())
                                {
                                    @Html.AntiForgeryToken()

                                    <div class="form-actions no-color">
                                        <input type="submit" value="Delete" class="btn btn-default" /> |
                                        @Html.ActionLink("Regresar a la Lista", "Index")
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



